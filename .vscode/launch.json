    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: vacumba light",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/vacumba_web_light.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Current File with args",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-b","86559","-e","86760","-c","1000"]
        }
    ]
